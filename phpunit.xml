<phpunit colors="true" strict="true">
    <filter>
        <whitelist processUncoveredFilesFromWhitelist="true">
            <directory suffix=".php">./src/</directory>
        </whitelist>
        <blacklist>
            <directory suffix=".php">./vendor/</directory>
            <directory>./documentation/</directory>
        </blacklist>
    </filter>
</phpunit>